{
  "collectionName": "components_page_media_and_texts",
  "info": {
    "displayName": "Media & Text",
    "icon": "brush"
  },
  "options": {},
  "attributes": {
    "Label": {
      "type": "string"
    },
    "Headline": {
      "type": "text"
    },
    "Text": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultMarkdown"
      }
    },
    "Media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "Reversed": {
      "type": "boolean"
    },
    "Background": {
      "type": "enumeration",
      "enum": [
        "LeftBottomGreen",
        "LeftBottomViolet",
        "RightTopGreen",
        "RightTopViolet"
      ]
    },
    "Links": {
      "type": "component",
      "component": "general.link",
      "repeatable": true
    }
  },
  "config": {}
}
