{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": false
    },
    "Slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "Title"
    },
    "Parent": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page"
    },
    "Elements": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "page.timeline",
        "page.tiles",
        "page.testimonials",
        "page.sponsors",
        "page.media-and-text",
        "page.media-and-text-list",
        "page.location",
        "page.intro",
        "page.hero",
        "page.gallery",
        "page.faq",
        "page.employees",
        "page.contact",
        "page.benefits",
        "page.banner"
      ]
    },
    "SEO": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "openGraph": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.open-graph",
      "repeatable": false
    }
  }
}
